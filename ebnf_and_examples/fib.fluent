notzero <- 1;

fib <- (n) {
	notzero <- 0;
	one <- 1;
  match n {
    0 -> notzero;
    1 -> one;
    _ -> add(fib(sub(n 1)) fib(sub(n 2)));
  };
};

fromTo <- (i n x) {
	x(i);
	match i {
		n -> NULL;
		_ -> fromTo(add(i 1) n x);
	};
};

printFib <- (n) {
	print(fib(n));
};

fromTo(0 8 printFib);

